
 <section class="compareSection" style="background-color:{{ section.settings.sectionBg }};" >
  <div class="page-width " >
    
      
    {% if section.settings.title %}
        <div class="titleHeader">
          <h2>{{ section.settings.title }}</h2>
        </div>
    {% endif %}

    


      <div class="compareTable">

        <table>
          <thead>
          <tr>
            <th></th>
            <th>
              {% if section.settings.logo %} 
                  <img src="{{ section.settings.logo | img_url:'1000x' }}" alt="{{ section.settings.alt }}" />  
              {% endif %}
            </th>
            <th>
              <strong>{{ section.settings.title1 }}</strong>
            </th>
             
          </tr>
            </thead>

          <tbody>
         {% for block in section.blocks %}
           {% assign title = block.settings.title %}
           {% assign column1 = block.settings.select1 %}
           {% assign column2 = block.settings.select2 %} 

            <tr>
              <td data-label=""><span>{{ title }}</span></td>
              <td data-label="MyPabl"> 
                 {% if column1 == "mark" %}
                   <svg height="100%" width="100%" xmlns="http://www.w3.org/2000/svg" classname="w-6 h-6" viewBox="0 0 512 512" fill="#00D832">
                    <path fill="#00D832" strokelinecap="round" strokelinejoin="round" d="M504 256c0 136.967-111.033 248-248 248S8 392.967 8 256 119.033 8 256 8s248 111.033 248 248zM227.314 387.314l184-184c6.248-6.248 6.248-16.379 0-22.627l-22.627-22.627c-6.248-6.249-16.379-6.249-22.628 0L216 308.118l-70.059-70.059c-6.248-6.248-16.379-6.248-22.628 0l-22.627 22.627c-6.248 6.248-6.248 16.379 0 22.627l104 104c6.249 6.249 16.379 6.249 22.628.001z"></path>
                   </svg>
                {% else %}
                   <svg height="100%" width="100%" xmlns="http://www.w3.org/2000/svg" classname="w-6 h-6" viewBox="0 0 512 512" fill="#F20000">
                    <path fill="#F20000" strokelinecap="round" strokelinejoin="round" d="M256 8C119 8 8 119 8 256s111 248 248 248 248-111 248-248S393 8 256 8zm0 448c-110.5 0-200-89.5-200-200S145.5 56 256 56s200 89.5 200 200-89.5 200-200 200zm101.8-262.2L295.6 256l62.2 62.2c4.7 4.7 4.7 12.3 0 17l-22.6 22.6c-4.7 4.7-12.3 4.7-17 0L256 295.6l-62.2 62.2c-4.7 4.7-12.3 4.7-17 0l-22.6-22.6c-4.7-4.7-4.7-12.3 0-17l62.2-62.2-62.2-62.2c-4.7-4.7-4.7-12.3 0-17l22.6-22.6c4.7-4.7 12.3-4.7 17 0l62.2 62.2 62.2-62.2c4.7-4.7 12.3-4.7 17 0l22.6 22.6c4.7 4.7 4.7 12.3 0 17z"></path>
                   </svg>
                {% endif %} 
              </td>
              <td data-label="{{ section.settings.title1 }}"> 
                 {% if column2 == "mark" %}
                   <svg height="100%" width="100%" xmlns="http://www.w3.org/2000/svg" classname="w-6 h-6" viewBox="0 0 512 512" fill="#00D832">
                    <path fill="#00D832" strokelinecap="round" strokelinejoin="round" d="M504 256c0 136.967-111.033 248-248 248S8 392.967 8 256 119.033 8 256 8s248 111.033 248 248zM227.314 387.314l184-184c6.248-6.248 6.248-16.379 0-22.627l-22.627-22.627c-6.248-6.249-16.379-6.249-22.628 0L216 308.118l-70.059-70.059c-6.248-6.248-16.379-6.248-22.628 0l-22.627 22.627c-6.248 6.248-6.248 16.379 0 22.627l104 104c6.249 6.249 16.379 6.249 22.628.001z"></path>
                   </svg>
                {% else %}
                   <svg height="100%" width="100%" xmlns="http://www.w3.org/2000/svg" classname="w-6 h-6" viewBox="0 0 512 512" fill="#F20000">
                    <path fill="#F20000" strokelinecap="round" strokelinejoin="round" d="M256 8C119 8 8 119 8 256s111 248 248 248 248-111 248-248S393 8 256 8zm0 448c-110.5 0-200-89.5-200-200S145.5 56 256 56s200 89.5 200 200-89.5 200-200 200zm101.8-262.2L295.6 256l62.2 62.2c4.7 4.7 4.7 12.3 0 17l-22.6 22.6c-4.7 4.7-12.3 4.7-17 0L256 295.6l-62.2 62.2c-4.7 4.7-12.3 4.7-17 0l-22.6-22.6c-4.7-4.7-4.7-12.3 0-17l62.2-62.2-62.2-62.2c-4.7-4.7-4.7-12.3 0-17l22.6-22.6c4.7-4.7 12.3-4.7 17 0l62.2 62.2 62.2-62.2c4.7-4.7 12.3-4.7 17 0l22.6 22.6c4.7 4.7 4.7 12.3 0 17z"></path>
                   </svg>
                {% endif %} 
              </td>
               
               
            </tr>
           
           
          {% endfor %}
            </tbody>
          
        </table>
     
     
       
     
      </div>
    
  </div>
     
    
</section>

{% schema %}
  {
    "name": "Compare Table", 
    "settings": [
      {
        "type":"text",
        "id":"title",
        "label":"Title",
        "default":"On This!"
      } ,
      {
        "type":"color",
        "id":"sectionBg",
        "label":"Section background"
      },
      {
        "type":"image_picker",
        "id":"logo",
        "label":"Image"
      } ,
      {
        "type":"text",
        "id":"title1",
        "label":"Title",
        "default":"Cheap Knock Off"
      } 
       
    ],
    "blocks":[
      {
        "type":"compare",
        "name":"Compare",
        "settings":[
            {
              "type": "text",
              "id": "title",  
              "label": "Title",
              "default": "The Original Band!"
            },
            {
              "type": "select",
              "id": "select1",  
              "label": "Mark/Unmark for mypadl",
              "default":"mark",
              "options":[
                {"value":"mark","label":"Mark"},
                {"value":"unmark","label":"Unmark"}
              ] 
            },
            {
              "type": "select",
              "id": "select2",  
              "label": "Mark/Unmark for Other",
              "default":"unmark",
              "options":[
                {"value":"mark","label":"Mark"},
                {"value":"unmark","label":"Unmark"}
              ] 
            } 
             
        ]
      }
    ],
       
    "presets":[
      {
        "name":"Compare Table"
      }
    ]
  }
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

{% javascript %}
  
 
  
{% endjavascript %}